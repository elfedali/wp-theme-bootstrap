{
	"browserSyncOptions": {
		"proxy": "localhost:10006/",
		"port": 4000,
		"notify": false,
		"open": true
	},
	"browserSyncWatchFiles": ["./assets/css/*.min.css", "./assets/js/*.min.js", "./**/*.php"],
	"paths": {
		"js": "./assets/js",
		"css": "./assets/css",
		"img": "./assets/img",
		"imgsrc": "./src/img",
		"sass": "./sass/**/*.scss",
		"node": "./node_modules/",
		"dev": "./src",
		"dist": "./dist",
		"distprod": "./dist-product",
		"vendor": ""
	}
}
 